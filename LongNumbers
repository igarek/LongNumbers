#include<stdio.h>
#include<conio.h>
#include<stdlib.h>
#include<iostream>

using namespase std;
class LongNumbers
{
char * number;
int length;
public:
IntNumbers(char * numb="0")
{
length=strlen(numb);
number=newchar[length];
strcpy(number,numb);
} 
class LongNumbers
{
      private:
       
        int deg;
        double * mas;
        
      
      public:
        LongNumbers();// constructors
        LongNumbers(int deg, double * mas);
        LongNumbers(const LongNumbers &x);
        ~LongNumbers();// destructor
        int getdeg();
        void setdeg(int newdeg);
        double getmas();
        void setmas(double newmas);
       
};
